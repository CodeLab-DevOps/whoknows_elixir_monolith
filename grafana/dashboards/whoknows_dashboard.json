{
  "title": "WhoKnows Application Metrics",
  "tags": ["phoenix", "elixir", "whoknows", "production"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 3,
  "refresh": "10s",
  "uid": "whoknows-app-metrics",
  "editable": true,
  "graphTooltip": 0,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "panels": [
      {
        "id": 100,
        "title": "Total Users",
        "type": "stat",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 4,
          "h": 4
        },
        "targets": [
          {
            "expr": "whoknows_users_count",
            "legendFormat": "Users",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "blue" },
                { "value": 100, "color": "green" },
                { "value": 1000, "color": "yellow" }
              ]
            }
          }
        }
      },
      {
        "id": 101,
        "title": "Total Pages",
        "type": "stat",
        "gridPos": {
          "x": 4,
          "y": 0,
          "w": 4,
          "h": 4
        },
        "targets": [
          {
            "expr": "whoknows_pages_count",
            "legendFormat": "Pages",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "blue" },
                { "value": 100, "color": "green" },
                { "value": 1000, "color": "yellow" }
              ]
            }
          }
        }
      },
      {
        "id": 102,
        "title": "Search Rate",
        "type": "stat",
        "gridPos": {
          "x": 8,
          "y": 0,
          "w": 4,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(rate(whoknows_search_query_count[1m]))",
            "legendFormat": "Searches/s",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 1, "color": "yellow" },
                { "value": 5, "color": "orange" }
              ]
            }
          }
        }
      },
      {
        "id": 1,
        "title": "Request Rate",
        "type": "stat",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 4,
          "h": 4
        },
        "targets": [
          {
            "expr": "sum(rate(phoenix_endpoint_stop_duration_count[1m]))",
            "legendFormat": "req/s",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 50, "color": "yellow" },
                { "value": 100, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "title": "Error Rate",
        "type": "stat",
        "gridPos": {
          "x": 16,
          "y": 0,
          "w": 4,
          "h": 4
        },
        "targets": [
          {
            "expr": "100 * sum(rate(phoenix_endpoint_stop_duration_count{status=~\"4..|5..\"}[1m])) / sum(rate(phoenix_endpoint_stop_duration_count[1m]))",
            "legendFormat": "Error Rate",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "decimals": 2,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 1, "color": "yellow" },
                { "value": 5, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Avg Response Time",
        "type": "stat",
        "gridPos": {
          "x": 20,
          "y": 0,
          "w": 4,
          "h": 4
        },
        "targets": [
          {
            "expr": "rate(phoenix_endpoint_stop_duration_sum[1m]) / rate(phoenix_endpoint_stop_duration_count[1m]) / 1000000",
            "legendFormat": "Avg Response",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "justifyMode": "auto",
          "orientation": "auto",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"]
          }
        },
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "decimals": 0,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": null, "color": "green" },
                { "value": 200, "color": "yellow" },
                { "value": 500, "color": "red" }
              ]
            }
          }
        }
      },
      {
        "id": 103,
        "title": "Search Queries by Language",
        "type": "timeseries",
        "gridPos": {
          "x": 0,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (language) (rate(whoknows_search_query_count[1m]))",
            "legendFormat": "{{language}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ops",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "showPoints": "never",
              "spanNulls": true,
              "stacking": { "mode": "normal" }
            }
          }
        },
        "options": {
          "tooltip": { "mode": "multi", "sort": "desc" },
          "legend": { "displayMode": "table", "placement": "right", "calcs": ["lastNotNull", "sum"] }
        }
      },
      {
        "id": 104,
        "title": "Total Searches Over Time",
        "type": "timeseries",
        "gridPos": {
          "x": 12,
          "y": 4,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum(whoknows_search_query_count)",
            "legendFormat": "Total Searches",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 20,
              "showPoints": "auto",
              "spanNulls": true
            }
          }
        },
        "options": {
          "tooltip": { "mode": "single" },
          "legend": { "displayMode": "list", "placement": "bottom" }
        }
      },
      {
        "id": 5,
        "title": "HTTP Requests by Status Code",
        "type": "timeseries",
        "gridPos": {
          "x": 0,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "sum by (status) (rate(phoenix_endpoint_stop_duration_count[1m]))",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "reqps",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 10,
              "showPoints": "never",
              "spanNulls": true
            }
          },
          "overrides": [
            {
              "matcher": { "id": "byRegexp", "options": "2.." },
              "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "green" } }]
            },
            {
              "matcher": { "id": "byRegexp", "options": "4.." },
              "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "yellow" } }]
            },
            {
              "matcher": { "id": "byRegexp", "options": "5.." },
              "properties": [{ "id": "color", "value": { "mode": "fixed", "fixedColor": "red" } }]
            }
          ]
        },
        "options": {
          "tooltip": { "mode": "multi", "sort": "desc" },
          "legend": { "displayMode": "table", "placement": "right", "calcs": ["lastNotNull", "max"] }
        }
      },
      {
        "id": 6,
        "title": "Response Time Distribution (p50, p95, p99)",
        "type": "timeseries",
        "gridPos": {
          "x": 12,
          "y": 12,
          "w": 12,
          "h": 8
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.50, sum(rate(phoenix_endpoint_stop_duration_bucket[1m])) by (le)) / 1000000",
            "legendFormat": "p50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, sum(rate(phoenix_endpoint_stop_duration_bucket[1m])) by (le)) / 1000000",
            "legendFormat": "p95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, sum(rate(phoenix_endpoint_stop_duration_bucket[1m])) by (le)) / 1000000",
            "legendFormat": "p99",
            "refId": "C"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "lineWidth": 2,
              "fillOpacity": 0,
              "showPoints": "never",
              "spanNulls": true
            }
          }
        },
        "options": {
          "tooltip": { "mode": "multi", "sort": "desc" },
          "legend": { "displayMode": "table", "placement": "right", "calcs": ["lastNotNull", "max"] }
        }
      },
